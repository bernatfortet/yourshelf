= form_for @user_band do |f|
  -if @user_band.errors.any?
    #error_explanation
      %h2= "#{pluralize(@user_band.errors.count, "error")} prohibited this user_band from being saved:"
      %ul
        - @user_band.errors.full_messages.each do |msg|
          %li= msg

  .field
    = f.label :user_id
    = f.number_field :user_id
  .field
    = f.label :band_id
    = f.number_field :band_id
  .actions
    = f.submit 'Save'
