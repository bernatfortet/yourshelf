= form_for @album_song do |f|
  -if @album_song.errors.any?
    #error_explanation
      %h2= "#{pluralize(@album_song.errors.count, "error")} prohibited this album_song from being saved:"
      %ul
        - @album_song.errors.full_messages.each do |msg|
          %li= msg

  .field
    = f.label :album_id
    = f.number_field :album_id
  .field
    = f.label :song_id
    = f.number_field :song_id
  .actions
    = f.submit 'Save'
