= form_for @album_genre do |f|
  -if @album_genre.errors.any?
    #error_explanation
      %h2= "#{pluralize(@album_genre.errors.count, "error")} prohibited this album_genre from being saved:"
      %ul
        - @album_genre.errors.full_messages.each do |msg|
          %li= msg

  .field
    = f.label :album_id
    = f.number_field :album_id
  .field
    = f.label :genre_id
    = f.number_field :genre_id
  .actions
    = f.submit 'Save'
