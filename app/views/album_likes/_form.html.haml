= form_for @album_like do |f|
  -if @album_like.errors.any?
    #error_explanation
      %h2= "#{pluralize(@album_like.errors.count, "error")} prohibited this album_like from being saved:"
      %ul
        - @album_like.errors.full_messages.each do |msg|
          %li= msg

  .field
    = f.label :user_id
    = f.number_field :user_id
  .field
    = f.label :album_id
    = f.number_field :album_id
  .actions
    = f.submit 'Save'
