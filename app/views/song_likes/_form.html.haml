= form_for @song_like do |f|
  -if @song_like.errors.any?
    #error_explanation
      %h2= "#{pluralize(@song_like.errors.count, "error")} prohibited this song_like from being saved:"
      %ul
        - @song_like.errors.full_messages.each do |msg|
          %li= msg

  .field
    = f.label :user_id
    = f.number_field :user_id
  .field
    = f.label :song_id
    = f.number_field :song_id
  .actions
    = f.submit 'Save'
